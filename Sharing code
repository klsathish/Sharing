 public async Task<List<LoginActivityResults>> GetLoginActivity(DateFilter model) => await _dbContext.LoginActivityResults.FromSqlRaw("EXEC [dbo].[SP_GetLoginActivity] {0},{1}",
     model.FromDate,
     model.ToDate).ToListAsync();
 public async Task<List<UserActivityResults>> GetLoginUserActivity(DateFilter model) => await _dbContext.UserActivityResults.FromSqlRaw("EXEC [dbo].[SP_GetLoginUserActivity] {0},{1}",
     model.FromDate,
     model.ToDate
     ).ToListAsync();


 public async Task InsertUserActivity(InsertUserActivity insertUserActivity)
 {
     await _dbContext.Database.ExecuteSqlRawAsync("EXEC [dbo].[SP_Insert_Update_ActivityTracker] {0},{1},{2},{3},{4}", insertUserActivity.ActivityTrackerID,
         insertUserActivity.ScreenID, insertUserActivity.FunctionID, insertUserActivity.UserActionID, insertUserActivity.CreatedBy);
 }
