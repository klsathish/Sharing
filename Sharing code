
SELECT  
    s.name AS SchemaName,
    p.name AS ProcedureName,
    prm.name AS ParameterName,
    CASE 
        WHEN prm.is_output = 1 THEN 'OUTPUT'
        ELSE 'INPUT'
    END AS ParameterType,
    TYPE_NAME(prm.user_type_id) AS DataType,
    prm.max_length AS MaxLength
FROM sys.procedures p
JOIN sys.schemas s ON p.schema_id = s.schema_id
LEFT JOIN sys.parameters prm ON p.object_id = prm.object_id
ORDER BY p.name, prm.parameter_id;





--                revenueFilterReq.TypeID,
                revenueFilterReq.CompanyCode,
                revenueFilterReq.ProfitCenterID,
                revenueFilterReq.FromDate,
                revenueFilterReq.ToDate,
                revenueFilterReq.SBU,
                revenueFilterReq.BU).ToListAsync();



