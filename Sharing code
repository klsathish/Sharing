public async Task<List<UnitOfMeasure>> GetUnitOfMeasure()
    => await _dbContext.UnitOfMeasure
        .FromSqlRaw("EXEC [dbo].[SP_GetUnitofMeasure]")
        .AsNoTracking()
        .ToListAsync();

