 public class ProductionRepository :IProductionRepository
 {
     private readonly SFDbContext _dbContext;
     public ProductionRepository(SFDbContext dbContext)
     {
         _dbContext = dbContext;
     }
     public async Task<List<ProductionMap>> GetProductionMapDetails(RevenueFilterReq revenueFilterRe)
     {
         return await _dbContext.ProductionMap.FromSqlRaw("EXEC [dbo].[SP_GetProductionMapDetails] {0},{1},{2},{3},{4},{5},{6}",
             revenueFilterRe.TypeID, 
             revenueFilterRe.CompanyCode, 
             revenueFilterRe.ProfitCenterID, 
             revenueFilterRe.FromDate, 
             revenueFilterRe.ToDate,
             revenueFilterRe.SBU,
             revenueFilterRe.BU).ToListAsync();
     }
     public async Task<List<HistoricalSaving>> GetHistoricalSavings(RevenueFilterReq revenueFilterRe)
     {
         return await _dbContext.HistoricalSaving.FromSqlRaw("EXEC [dbo].[SP_GetHistoricalSavings] {0},{1},{2},{3},{4},{5},{6}",
             revenueFilterRe.TypeID,
             revenueFilterRe.CompanyCode,
             revenueFilterRe.ProfitCenterID,
             revenueFilterRe.FromDate,
             revenueFilterRe.ToDate,
             revenueFilterRe.SBU,
             revenueFilterRe.BU).ToListAsync();
     }
 }
