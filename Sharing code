SELECT 
    OBJECT_NAME(object_id) AS ProcedureName,
    column_ordinal AS ColumnOrder,
    name AS ColumnName,
    system_type_name AS DataType
FROM sys.dm_exec_describe_first_result_set_for_object(NULL, 0) -- This is a placeholder
WHERE object_id IN (
    SELECT object_id 
    FROM sys.objects 
    WHERE type = 'P' 
    AND name IN (
        'SP_GetAIForecastValue', 
        'SP_GetInventoryDIO', 
        'SP_GetFGClosingInventoryBalanceChartData',
        -- Add your other procedure names here or remove the IN clause to get ALL
        'SP_GetAuthenticatedUserDetails'
    )
)
ORDER BY ProcedureName, ColumnOrder;

SELECT 
    p.name AS ProcedureName,
    r.name AS ColumnName,
    r.system_type_name AS DataType
FROM sys.procedures AS p
CROSS APPLY sys.dm_exec_describe_first_result_set_for_object(p.object_id, 0) AS r
ORDER BY p.name, r.column_ordinal;



