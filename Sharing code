public async Task<List<RawMaterialsGoodsReceiptChart>> GetRawMaterialsGoodsReceiptChartData(ChartDataFilterReq chartDataFilterReq)
{
    return await _dbContext.RawMaterialsGoodsReceiptChart
        .FromSqlRaw(@"EXEC [dbo].[SP_GetRawMaterialsGoodsReceiptChartAIEnhancedForecastData]
                        @CurrencyType = @CurrencyType,
                        @Metrics = @Metrics,
                        @RegionCode = @RegionCode,
                        @CountryCode = @CountryCode,
                        @CompanyCode = @CompanyCode,
                        @ProfitCenterID = @ProfitCenterID,
                        @PlantID = @PlantID,
                        @SbuName = @SbuName,
                        @BuName = @BuName,
                        @FromDate = @FromDate,
                        @ToDate = @ToDate,
                        @UnitOfMeasure = @UnitOfMeasure",
            new SqlParameter("@CurrencyType", chartDataFilterReq.CurrencyType),
            new SqlParameter("@Metrics", chartDataFilterReq.Metrics),
            new SqlParameter("@RegionCode", chartDataFilterReq.RegionCode),
            new SqlParameter("@CountryCode", chartDataFilterReq.CountryCode),
            new SqlParameter("@CompanyCode", chartDataFilterReq.CompanyCode),
            new SqlParameter("@ProfitCenterID", chartDataFilterReq.ProfitCenterID),
            new SqlParameter("@PlantID", chartDataFilterReq.PlantID),
            new SqlParameter("@SbuName", chartDataFilterReq.SbuName),
            new SqlParameter("@BuName", chartDataFilterReq.BuName),
            new SqlParameter("@FromDate", chartDataFilterReq.FromDate),
            new SqlParameter("@ToDate", chartDataFilterReq.ToDate),
            new SqlParameter("@UnitOfMeasure", chartDataFilterReq.UnitOfMeasure))
        .ToListAsync();
}

