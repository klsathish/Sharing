public async Task<List<InventoryAIForecastValue>> GetInventoryAIForecastValue(InventoryAIForecastReq r)
    => await _dbContext.InventoryAIForecastValue
        .FromSqlRaw(
            @"EXEC [dbo].[SP_GetInventoryAIForecastValue]
              @RegionCode      = @RegionCode,
              @CountryCode     = @CountryCode,
              @CompanyCode     = @CompanyCode,
              @ProfitCenterID  = @ProfitCenterID,
              @PlantID         = @PlantID,
              @SbuName         = @SbuName,
              @BuName          = @BuName,
              @FromDate        = @FromDate,
              @ToDate          = @ToDate",
            new SqlParameter("@RegionCode", r.RegionCode),
            new SqlParameter("@CountryCode", r.CountryCode),
            new SqlParameter("@CompanyCode", r.CompanyCode),
            new SqlParameter("@ProfitCenterID", r.ProfitCenterID),
            new SqlParameter("@PlantID", r.PlantID),
            new SqlParameter("@SbuName", r.SbuName),
            new SqlParameter("@BuName", r.BuName),
            new SqlParameter("@FromDate", r.FromDate),
            new SqlParameter("@ToDate", r.ToDate))
        .AsNoTracking()
        .ToListAsync();
