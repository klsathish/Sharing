	[CalYearMonth] [int] NULL,
	[CalYear] [int] NULL,
	[CalMonth] [int] NULL,
	[InventoryType] [varchar](100) NULL,
	[CompanyCode] [varchar](100) NULL,
	[CompanyName] [varchar](100) NULL,
	[OpeningInventory_Volume_In_MT] [float] NULL,
	[ClosingInventory_Volume_In_MT] [float] NULL,
	[OpeningInventory_Value_In_MillionSAR] [float] NULL,
	[OpeningInventory_Value_In_MillionUSD] [float] NULL,
	[ClosingInventory_Value_In_MillionSAR] [float] NULL,
	[ClosingInventory_Value_In_MillionUSD] [float] NULL,
	[volume_Production_Output_In_MT] [float] NULL,
	[volume_STO_IN_In_MT] [float] NULL,
	[volume_STO_OUT_In_MT] [float] NULL,
	[volume_Transfers_IN_In_MT] [float] NULL,
	[volume_Transfers_OUT_In_MT] [float] NULL,
	[volume_Sale_In_MT] [float] NULL,
	[volume_Consumed_in_production_In_MT] [float] NULL,
	[volume_Adjustments_In_MT] [float] NULL,
	[volume_Purchase_In_MT] [float] NULL,
	[value_Production_Output_In_MillionSAR] [float] NULL,
	[value_Production_Output_In_MillionUSD] [float] NULL,
	[value_STO_IN_In_MillionSAR] [float] NULL,
	[value_STO_IN_In_MillionUSD] [float] NULL,
	[value_STO_OUT_In_MillionSAR] [float] NULL,
	[value_STO_OUT_In_MillionUSD] [float] NULL,
	[value_Transfers_IN_In_MillionSAR] [float] NULL,
	[value_Transfers_IN_In_MillionUSD] [float] NULL,
	[value_Transfers_OUT_In_MillionSAR] [float] NULL,
	[value_Transfers_OUT_In_MillionUSD] [float] NULL,
	[value_Sale_In_MillionSAR] [float] NULL,
	[value_Sale_In_MillionUSD] [float] NULL,
	[value_Consumed_in_production_In_MillionSAR] [float] NULL,
	[value_Consumed_in_production_In_MillionUSD] [float] NULL,
	[value_Adjustments_In_MillionSAR] [float] NULL,
	[value_Adjustments_In_MillionUSD] [float] NULL,
	[value_Purchase_In_MillionSAR] [float] NULL,
	[value_Purchase_In_MillionUSD] [float] NULL,
	[CreatedDate] [datetime] NULL,
	[Profit_Center] [varchar](10) NULL,
	[SBU] [varchar](10) NULL,
	[BU] [varchar](10) NULL,
