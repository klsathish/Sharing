	[movement_type_text] [varchar](25) NULL,
	[special_stock] [char](1) NULL,
	[special_stock_description] [varchar](25) NULL,
	[movement_indicator] [char](1) NULL,
	[movement_indicator_description] [varchar](100) NULL,
	[receipt_indicator] [char](1) NULL,
	[receipt_Indicator_description] [varchar](100) NULL,
	[consumption] [char](1) NULL,
	[consumption_description] [varchar](100) NULL,
	[classification_level1] [varchar](50) NULL,
	[classification_level2] [varchar](50) NULL,
	[classification_level3] [varchar](50) NULL,
	[master_data] [varchar](15) NULL,
PRIMARY KEY CLUSTERED 
(
	[RecID] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]
