public async Task<InventoryDIO?> GetInventoryDIO(InventoryFilterReq r)
    => (await _dbContext.InventoryDIO
        .FromSqlRaw(
            @"EXEC [dbo].[SP_GetInventoryDIO]
              @RegionCode=@RegionCode,@CountryCode=@CountryCode,@CompanyCode=@CompanyCode,
              @ProfitCenterID=@ProfitCenterID,@PlantID=@PlantID,
              @SbuName=@SbuName,@BuName=@BuName,@FromDate=@FromDate,@ToDate=@ToDate",
            new SqlParameter("@RegionCode", r.RegionCode),
            new SqlParameter("@CountryCode", r.CountryCode),
            new SqlParameter("@CompanyCode", r.CompanyCode),
            new SqlParameter("@ProfitCenterID", r.ProfitCenterID),
            new SqlParameter("@PlantID", r.PlantID),
            new SqlParameter("@SbuName", r.SbuName),
            new SqlParameter("@BuName", r.BuName),
            new SqlParameter("@FromDate", r.FromDate),
            new SqlParameter("@ToDate", r.ToDate))
        .AsNoTracking()
        .ToListAsync())
        .FirstOrDefault();

public async Task<List<ClosingInventoryBalanceChart>> GetClosingInventoryBalanceChart(ChartDataFilterReq r)
    => await _dbContext.ClosingInventoryBalanceChart
        .FromSqlRaw(
            @"EXEC [dbo].[SP_GetFGClosingInventoryBalanceChartData]
              @CurrencyType=@CurrencyType,@Metrics=@Metrics,@RegionCode=@RegionCode,
              @CountryCode=@CountryCode,@CompanyCode=@CompanyCode,@ProfitCenterID=@ProfitCenterID,
              @PlantID=@PlantID,@SbuName=@SbuName,@BuName=@BuName,@FromDate=@FromDate,@ToDate=@ToDate",
            new SqlParameter("@CurrencyType", r.CurrencyType),
            new SqlParameter("@Metrics", r.Metrics),
            new SqlParameter("@RegionCode", r.RegionCode),
            new SqlParameter("@CountryCode", r.CountryCode),
            new SqlParameter("@CompanyCode", r.CompanyCode),
            new SqlParameter("@ProfitCenterID", r.ProfitCenterID),
            new SqlParameter("@PlantID", r.PlantID),
            new SqlParameter("@SbuName", r.SbuName),
            new SqlParameter("@BuName", r.BuName),
            new SqlParameter("@FromDate", r.FromDate),
            new SqlParameter("@ToDate", r.ToDate))
        .AsNoTracking()
        .ToListAsync();

