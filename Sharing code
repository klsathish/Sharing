public class DashboardRepository : IDashboardRepository
{
    private readonly WCIDbContext _dbContext;

    public DashboardRepository(WCIDbContext dbContext)
    {
        _dbContext = dbContext;
    }

    public async Task<List<BaseFilterResult>> GetBaseFilter(FilterModel model)
        => await _dbContext.BaseFilterResult
            .FromSqlRaw(
                @"EXEC [dbo].[SP_GetBaseFilters]
                    @RoleID = @RoleID,
                    @RegionCode = @RegionCode,
                    @CountryCode = @CountryCode",
                new SqlParameter("@RoleID", model.RoleID),
                new SqlParameter("@RegionCode", model.RegionCode),
                new SqlParameter("@CountryCode", model.CountryCode))
            .AsNoTracking()
            .ToListAsync();

    public async Task<List<MoreFiltersSBUandBUResult>> GetMoreFiltersSBUandBU(FilterModel model)
        => await _dbContext.MoreFiltersSBUandBUResult
            .FromSqlRaw(
                @"EXEC [dbo].[SP_GetMoreFiltersSBUandBU]
                    @SbuName = @SbuName,
                    @BuName = @BuName,
                    @CompanyID = @CompanyID",
                new SqlParameter("@SbuName", model.SbuName),
                new SqlParameter("@BuName", model.BuName),
                new SqlParameter("@CompanyID", model.CompanyID))
            .AsNoTracking()
            .ToListAsync();

    public async Task<List<MoreFiltersProfitCenterResult>> GetMoreFiltersProfitCenter(FilterModel model)
        => await _dbContext.MoreFiltersProfitCenterResult
            .FromSqlRaw(
                @"EXEC [dbo].[SP_GetMoreFiltersProfitCenter]
                    @CompanyID = @CompanyID,
                    @SbuName = @SbuName,
                    @BuName = @BuName",
                new SqlParameter("@CompanyID", model.CompanyID),
                new SqlParameter("@SbuName", model.SbuName),
                new SqlParameter("@BuName", model.BuName))
            .AsNoTracking()
            .ToListAsync();

    public async Task<List<MoreFiltersPlantResult>> GetMoreFiltersPlant(FilterModel model)
        => await _dbContext.MoreFiltersPlantResult
            .FromSqlRaw(
                @"EXEC [dbo].[SP_GetMoreFiltersPlant]
                    @CompanyID = @CompanyID,
                    @ProfitCenterID = @ProfitCenterID",
                new SqlParameter("@CompanyID", model.CompanyID),
                new SqlParameter("@ProfitCenterID", model.ProfitCenterID))
            .AsNoTracking()
            .ToListAsync();

    public async Task<InventoryDiffValue?> GetInventoryDifferenceValue(InventoryFilterReq r)
        => (await _dbContext.InventoryDiffValue
            .FromSqlRaw(
                @"EXEC [dbo].[SP_GetDiffrencesValues]
                    @TypeID=@TypeID,@RegionCode=@RegionCode,@CountryCode=@CountryCode,
                    @CompanyCode=@CompanyCode,@ProfitCenterID=@ProfitCenterID,@PlantID=@PlantID,
                    @SbuName=@SbuName,@BuName=@BuName,@FromDate=@FromDate,@ToDate=@ToDate",
                new SqlParameter("@TypeID", r.TypeID),
                new SqlParameter("@RegionCode", r.RegionCode),
                new SqlParameter("@CountryCode", r.CountryCode),
                new SqlParameter("@CompanyCode", r.CompanyCode),
                new SqlParameter("@ProfitCenterID", r.ProfitCenterID),
                new SqlParameter("@PlantID", r.PlantID),
                new SqlParameter("@SbuName", r.SbuName),
                new SqlParameter("@BuName", r.BuName),
                new SqlParameter("@FromDate", r.FromDate),
                new SqlParameter("@ToDate", r.ToDate))
            .AsNoTracking()
            .ToListAsync())
            .FirstOrDefault();

    public async Task<InventoryDiffVolume?> GetInventoryDifferenceVolume(InventoryFilterReq r)
        => (await _dbContext.InventoryDiffVolume
            .FromSqlRaw(
                @"EXEC [dbo].[SP_GetDiffrencesVolumes]
                    @TypeID=@TypeID,@RegionCode=@RegionCode,@CountryCode=@CountryCode,
                    @CompanyCode=@CompanyCode,@ProfitCenterID=@ProfitCenterID,@PlantID=@PlantID,
                    @SbuName=@SbuName,@BuName=@BuName,@FromDate=@FromDate,@ToDate=@ToDate",
                new SqlParameter("@TypeID", r.TypeID),
                new SqlParameter("@RegionCode", r.RegionCode),
                new SqlParameter("@CountryCode", r.CountryCode),
                new SqlParameter("@CompanyCode", r.CompanyCode),
                new SqlParameter("@ProfitCenterID", r.ProfitCenterID),
                new SqlParameter("@PlantID", r.PlantID),
                new SqlParameter("@SbuName", r.SbuName),
                new SqlParameter("@BuName", r.BuName),
                new SqlParameter("@FromDate", r.FromDate),
                new SqlParameter("@ToDate", r.ToDate))
            .AsNoTracking()
            .ToListAsync())
            .FirstOrDefault();

    public async Task<InventoryDIO?> GetInventoryDIO(InventoryFilterReq r)
        => (await _dbContext.InventoryDIO
            .FromSqlRaw(
                @"EXEC [dbo].[SP_GetInventoryDIO]
                    @RegionCode=@RegionCode,@CountryCode=@CountryCode,@CompanyCode=@CompanyCode,
                    @ProfitCenterID=@ProfitCenterID,@PlantID=@PlantID,@SbuName=@SbuName,
                    @BuName=@BuName,@FromDate=@FromDate,@ToDate=@ToDate",
                new SqlParameter("@RegionCode", r.RegionCode),
                new SqlParameter("@CountryCode", r.CountryCode),
                new SqlParameter("@CompanyCode", r.CompanyCode),
                new SqlParameter("@ProfitCenterID", r.ProfitCenterID),
                new SqlParameter("@PlantID", r.PlantID),
                new SqlParameter("@SbuName", r.SbuName),
                new SqlParameter("@BuName", r.BuName),
                new SqlParameter("@FromDate", r.FromDate),
                new SqlParameter("@ToDate", r.ToDate))
            .AsNoTracking()
            .ToListAsync())
            .FirstOrDefault();

    public async Task<List<UnitOfMeasure>> GetUnitOfMeasure()
        => await _dbContext.UnitOfMeasure
            .FromSqlRaw("EXEC [dbo].[SP_GetUnitofMeasure]")
            .AsNoTracking()
            .ToListAsync();
}


public async Task<List<FinishedGoodsDemandComparison>> GetFinishedGoodsDemandComparisonValue(InventoryFilterReq r)
    => await _dbContext.FinishedGoodsDemandComparison
        .FromSqlRaw(
            @"EXEC [dbo].[SP_GetFinishedGoodsDemandGraphComparisonData]
                @CompanyCode=@CompanyCode,
                @ProfitCenterID=@ProfitCenterID,
                @PlantID=@PlantID,
                @SbuName=@SbuName,
                @BuName=@BuName,
                @FromDate=@FromDate,
                @ToDate=@ToDate",
            new SqlParameter("@CompanyCode", r.CompanyCode),
            new SqlParameter("@ProfitCenterID", r.ProfitCenterID),
            new SqlParameter("@PlantID", r.PlantID),
            new SqlParameter("@SbuName", r.SbuName),
            new SqlParameter("@BuName", r.BuName),
            new SqlParameter("@FromDate", r.FromDate),
            new SqlParameter("@ToDate", r.ToDate))
        .AsNoTracking()
        .ToListAsync();

public async Task<List<FinishedGoodsProductionComparison>> GetFinishedGoodsProductionComparisonValue(InventoryFilterReq r)
    => await _dbContext.FinishedGoodsProductionComparison
        .FromSqlRaw(
            @"EXEC [dbo].[SP_GetFinishedGoodsProductionGraphComparisonData]
                @CompanyCode=@CompanyCode,
                @ProfitCenterID=@ProfitCenterID,
                @PlantID=@PlantID,
                @SbuName=@SbuName,
                @BuName=@BuName,
                @FromDate=@FromDate,
                @ToDate=@ToDate",
            new SqlParameter("@CompanyCode", r.CompanyCode),
            new SqlParameter("@ProfitCenterID", r.ProfitCenterID),
            new SqlParameter("@PlantID", r.PlantID),
            new SqlParameter("@SbuName", r.SbuName),
            new SqlParameter("@BuName", r.BuName),
            new SqlParameter("@FromDate", r.FromDate),
            new SqlParameter("@ToDate", r.ToDate))
        .AsNoTracking()
        .ToListAsync();

public async Task<List<FinishedGoodsDemandChart>> GetFinishedGoodsDemandChartData(ChartDataFilterReq r)
    => await _dbContext.FinishedGoodsDemandChart
        .FromSqlRaw(
            @"EXEC [dbo].[SP_GetFinishedGoodsDemandChartAIEnhancedForecastData]
                @CurrencyType=@CurrencyType,@Metrics=@Metrics,@RegionCode=@RegionCode,
                @CountryCode=@CountryCode,@CompanyCode=@CompanyCode,@ProfitCenterID=@ProfitCenterID,
                @PlantID=@PlantID,@SbuName=@SbuName,@BuName=@BuName,@FromDate=@FromDate,@ToDate=@ToDate",
            new SqlParameter("@CurrencyType", r.CurrencyType),
            new SqlParameter("@Metrics", r.Metrics),
            new SqlParameter("@RegionCode", r.RegionCode),
            new SqlParameter("@CountryCode", r.CountryCode),
            new SqlParameter("@CompanyCode", r.CompanyCode),
            new SqlParameter("@ProfitCenterID", r.ProfitCenterID),
            new SqlParameter("@PlantID", r.PlantID),
            new SqlParameter("@SbuName", r.SbuName),
            new SqlParameter("@BuName", r.BuName),
            new SqlParameter("@FromDate", r.FromDate),
            new SqlParameter("@ToDate", r.ToDate))
        .AsNoTracking()
        .ToListAsync();

